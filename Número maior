import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner ler = new Scanner(System.in);
        int a, b;
        System.out.println("Insira o primeiro num:");
        a = ler.nextInt();
        System.out.println("Insira o segundo num:");
        b = ler.nextInt();
        
        if (a > b) {
            System.out.printf("O numero %d e maior.", a);
        } else if (b > a) {
            System.out.printf("O numero %d e maior.", b);
        } else {
            System.out.printf("As duas variaveis possuem o mesmo valor.");
        }
        ler.close();
    }
}
